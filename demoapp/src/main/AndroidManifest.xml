<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
          package="com.billionsfinance.behaviorsdk.demo">

    <application
        android:name="com.billionsfinance.behaviorsdk.demo.MyApplication"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">

        <activity
            android:name="com.billionsfinance.behaviorsdk.demo.SampleActivity"
            android:label="@string/app_name">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <activity
            android:label="第二个activity"
            android:name="com.billionsfinance.behaviorsdk.demo.TestActivity">
        </activity>

        <activity
            android:label="WebView"
            android:name="com.billionsfinance.behaviorsdk.demo.WebActivity">
        </activity>

        <activity
            android:label="FragmentActivity"
            android:name="com.billionsfinance.behaviorsdk.demo.fragment.FragmentTestActivity">
        </activity>
    </application>

</manifest>